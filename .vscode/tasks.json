{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build: solution",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/changes.sln"
      ],
      "problemMatcher": "$msCompile",
